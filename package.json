{
  "name": "money-gaming-automation-test",
  "version": "1.0.0",
  "description": "Cypress cucumber BDD project for Money Gaming  ",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "test": "cypress run",
    "verify": "cypress verify",
    "cy:record": "cypress run --record --key cdb6ccb4-9476-4871-a722-94f02f6ab086",
    "cy:api": "cypress open --config-file ./cypress-api.json",
    "cy:ui": "cypress run --config-file ./cypress-ui.json",

    "cy:run": "cypress run --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "if exist allure-results rmdir /q /s allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos",
    "pretest": "npm run allure:clear",
    "test-report": "npm run cy:run || npm run posttest",
    "posttest": "npm run allure:report"

  },
  "author": "Sachin Jadhav",
  "license": "ISC",
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.4.0",
    "allure-commandline": "^2.13.8",
    "cypress": "^4.12.1",
    "cypress-cucumber-preprocessor": "^2.0.1",
    "deepmerge": "4.2.2",
    "fs": "0.0.2",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "graceful-fs": "^4.2.4",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cucumber": "0.0.23",
    "gulp-run": "1.7.1",
    "gulp-sequence": "0.4.6",
    "mochawesome": "^6.2.2",
    "prettier": "^1.19.1",
    "xpath.js": "1.1.0",
    "yaml-merge": "0.0.2",
    "yargs": "11.0.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "cypress-xpath": "^1.4.0",
    "gulp": "^3.9.1",
    "js-yaml": "^3.10.0"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
